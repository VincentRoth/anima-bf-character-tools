{
  "name": "anima-beyond-fantasy-character-tools",
  "version": "0.9.2",
  "license": "cc-by-nc-sa-4.0",
  "description": "Angular character tools for table RPG Anima Beyond Fantasy",
  "repository": {
    "type": "git",
    "url": "https://github.com/VincentRoth/anima-bf-character-tools"
  },
  "engines": {
    "node": ">=12.x <=16.x"
  },
  "scripts": {
    "ng": "ng",
    "serve": "ng serve -o",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "npm run lint:tslint && npm run lint:prettier",
    "lint:tslint": "ng lint",
    "lint:prettier": "prettier --list-different --config ./.prettierrc \"**/*.*\"",
    "fix": "npm run fix:tslint && npm run lint:prettier",
    "fix:tslint": "npm run lint:tslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/pwa": "^14.2.1",
    "@angular/router": "^14.2.0",
    "@angular/service-worker": "^14.2.0",
    "lodash-es": "^4.17.21",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.1",
    "@angular/cli": "^14.2.1",
    "@angular/compiler-cli": "^14.2.0",
    "@angular/language-service": "^14.2.0",
    "@types/jasmine": "~4.3.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.4.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.6.4"
  }
}