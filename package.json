  },
  "engines": {
    "node": ">=10.x"
  },
  "scripts": {
    "start": "node server.js",
    "ng": "ng",
    "serve": "ng serve -o",
    "build": "ng build --prod",
    "prettier": "prettier --config ./.prettierrc \"**/*.*\"",
    "lint": "npm run lint:prettier && npm run lint:tslint",
    "lint:prettier": "npm run prettier -- --check",
    "lint:tslint": "ng lint",
    "fix": "npm run fix:prettier && npm run fix:tslint",
    "fix:prettier": "npm run prettier -- --write",
    "fix:tslint": "ng lint --fix",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.5",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
